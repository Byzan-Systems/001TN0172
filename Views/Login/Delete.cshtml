@model _001TN0172.Models.LoginMST
@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="../../plugins/fontawesome-free/css/all.min.css">
    <!-- icheck bootstrap -->
    <link rel="stylesheet" href="../../plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="../../dist/css/adminlte.min.css">
</head>
<div class="content-wrapper">


    <!-- general form elements disabled -->
    <div class="card card-warning">
        <div class="card-header">
            <h1 class="card-title">User Master</h1>
        </div>
        <!-- /.card-header -->
        <div class="card-body">
            <h2>Deleting Employee</h2>

            <h3>Would you like to continue?</h3>
            <div class="container p-3 my-3 border">
                <hr />
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.LoginID)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.LoginID)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.LoginName)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.LoginName)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.LoginType)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.LoginType)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Login_Enable)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Login_Enable )
                    </dd>
                   
                </dl>

                <form asp-action="Delete">
                    <input type="hidden" asp-for="LoginID" />
                    <input class="btn btn-primary" type="submit" value="Yes" />
                    <a class="btn btn-danger" asp-action="Index">No</a>
                </form>

            </div>
        </div>
        <!-- /.card-body -->
    </div>
    <!-- /.card -->

</div>
<!-- jQuery -->
<script src="../../plugins/jquery/jquery.min.js"></script>
<!-- Bootstrap 4 -->
<script src="../../plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
<!-- AdminLTE App -->
<script src="../../dist/js/adminlte.min.js"></script>